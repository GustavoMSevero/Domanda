<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gustavo.domanda">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BuscarActivity"
            android:label="@string/title_activity_buscar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UnidadeActivity" />
        <activity
            android:name=".ProfissionalActivity"
            android:label="@string/title_activity_profisional"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AgendamentoActivity"
            android:label="@string/title_activity_agendamento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EstabelecimentoActivity"
            android:label="@string/title_activity_estabelecimento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SobreActivity"
            android:label="@string/title_activity_sobre"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConsultarActivity"
            android:label="@string/title_activity_consultar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>